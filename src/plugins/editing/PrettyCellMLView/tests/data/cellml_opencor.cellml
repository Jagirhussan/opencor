<?xml version="1.0" encoding="utf-8"?>
<model name="cellml_1_1" cmeta:id="cellml_1_1" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
    <!--A comment can fit on one line...-->
    <!--A comment can also fit
on one, two or even
more lines...-->
    <units name="unitsx">
        <!--Comments can be within a unit definition...-->
        <unit units="unitx">
            <!--Comments can also be within a unit statement,
but if kept it will be serialised back in the
corresponding unit definition...-->
        </unit>
    </units>
    <component name="componentx">
        <!--Comments can be within a component definition...-->
        <variable name="variablex">
            <!--Comments can also be within a variable declaration,
but if kept it will be serialised back in the
corresponding component definition...-->
        </variable>
    </component>
    <group>
        <!--Comments can be within a group definition...-->
    </group>
    <connection>
        <!--Comments can be within a connection definition...-->
    </connection>
</model>
