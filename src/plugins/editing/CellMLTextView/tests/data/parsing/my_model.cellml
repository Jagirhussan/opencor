        <component cmeta:id="my_imported_component1_cmeta_id" component_ref="my_reference_component1" name="my_imported_component1"/>
        <component cmeta:id="my_imported_component2_cmeta_id" component_ref="my_reference_component2" name="my_imported_component2"/>
        <!-- In between comment...-->
        <units cmeta:id="my_imported_unit2_cmeta_id" name="my_imported_unit2" units_ref="my_reference_unit2"/>
        <!-- In between comment...-->
    </import>
    <!-- In between comment...-->
    <units base_units="yes" cmeta:id="my_base_unit_cmeta_id" name="my_base_unit"/>
    <!-- In between comment...-->
    <units cmeta:id="my_unit1_cmeta_id" name="my_unit1"/>
    <!-- In between comment...-->
    <units cmeta:id="my_unit2_cmeta_id" name="my_unit2">
        <!-- In between comment...-->
        <unit cmeta:id="second_cmeta_id" units="second"/>
        <!-- In between comment...-->
        <unit prefix="milli" units="some_unit"/>
        <!-- In between comment...-->
        <unit prefix="3" units="some_unit"/>
        <!-- In between comment...-->
        <unit prefix="+3" units="some_unit"/>
        <!-- In between comment...-->
        <unit prefix="-3" units="some_unit"/>
        <!-- In between comment...-->
        <unit exponent="3" units="some_unit"/>
        <!-- In between comment...-->
        <unit multiplier="3" units="some_unit"/>
        <!-- In between comment...-->
        <unit offset="3" units="some_unit"/>
        <!-- In between comment...-->
        <unit exponent="3" multiplier="+5" offset="-7" prefix="milli" units="some_unit"/>
        <!-- In between comment...-->
    </units>
    <!-- In between comment...-->
    <group>
        <relationship_ref relationship="containment"/>
        <!-- In between comment...-->
        <component_ref component="my_component1">
            <!-- In between comment...-->
            <component_ref component="my_component11">
                <!-- In between comment...-->
                <component_ref component="my_component111"/>
                <!-- In between comment...-->
                <component_ref component="my_component112"/>
                <!-- In between comment...-->
            </component_ref>
            <!-- In between comment...-->
        </component_ref>
        <!-- In between comment...-->
    </group>
    <!-- In between comment...-->
    <group>
        <relationship_ref name="my_containment" relationship="containment"/>
        <component_ref component="my_component1">
            <component_ref component="my_component12">
                <component_ref component="my_component121"/>
            </component_ref>
            <component_ref component="my_component13"/>
        </component_ref>
    </group>
    <!-- In between comment...-->
    <group>
        <relationship_ref relationship="encapsulation"/>
        <component_ref component="my_component11">
            <component_ref component="my_component111"/>
            <component_ref component="my_component112"/>
        </component_ref>
        <component_ref component="my_component12">
            <component_ref component="my_component121"/>
        </component_ref>
    </group>
    <!-- In between comment...-->
    <group>
        <relationship_ref relationship="encapsulation"/>
        <relationship_ref name="my_containment" relationship="containment"/>
        <component_ref component="my_component"/>
    </group>
    <!-- In between comment...-->
    <connection cmeta:id="my_connection_cmeta_id">
        <map_components cmeta:id="my_map_components_cmeta_id" component_1="my_component1" component_2="my_component2"/>
        <!-- In between comment...-->
        <map_variables cmeta:id="my_map_variables1_cmeta_id" variable_1="my_variable11" variable_2="my_variable21"/>
        <!-- In between comment...-->
        <map_variables cmeta:id="my_map_variables2_cmeta_id" variable_1="my_variable12" variable_2="my_variable22"/>
        <!-- In between comment...-->
        <map_variables cmeta:id="my_map_variables3_cmeta_id" variable_1="my_variable13" variable_2="my_variable23"/>
        <!-- In between comment...-->
    </connection>
</model>
